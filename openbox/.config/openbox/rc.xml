<?xml version="1.0"?>
<openbox_config xmlns="http://openbox.org/3.4/rc">
  <resistance>
    <strength>10</strength>
    <screen_edge_strength>20</screen_edge_strength>
  </resistance>
  <focus>
    <focusNew>yes</focusNew>
    <focusLast>yes</focusLast>
    <followMouse>no</followMouse>
    <underMouse>no</underMouse>
    <focusDelay>200</focusDelay>
    <raiseOnFocus>no</raiseOnFocus>
  </focus>
  <placement>
    <policy>Smart</policy>
    <center>yes</center>
    <monitor>Active</monitor>
  </placement>
  <theme>
    <name>thicc</name>
    <titleLayout></titleLayout>
    <cornerRadius>16</cornerRadius>
    <keepBorder>no</keepBorder>
    <animateIconify>no</animateIconify>
    <font place="ActiveWindow">
      <size>1</size>
    </font>
    <font place="InactiveWindow">
      <size>1</size>
    </font>
  </theme>
  <desktops>
    <number>6</number>
    <firstdesk>1</firstdesk>
    <names>
      <name>1</name>
      <name>2</name>
      <name>3</name>
      <name>4</name>
      <name>5</name>
      <name>6</name>
    </names>
    <popupTime>0</popupTime>
  </desktops>
  <resize>
    <drawContents>yes</drawContents>
    <popupShow>Never</popupShow>
    <popupPosition>Center</popupPosition>
    <popupFixedPosition>
      <x>10</x>
      <y>10</y>
    </popupFixedPosition>
  </resize>
  <margins>
    <top>0</top>
    <bottom>0</bottom>
    <left>0</left>
    <right>0</right>
  </margins>
  <keyboard>
    <chainQuitKey>C-g</chainQuitKey>
    <keybind key="C-Left">
      <action name="DesktopLeft">
	<wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="C-Right">
      <action name="DesktopRight">
	<wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="C-A-Left">
      <action name="SendToDesktopLeft">
	<wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="C-A-Right">
      <action name="SendToDesktopRight">
	<wrap>no</wrap>
      </action>
    </keybind>
    <keybind key="W-1">
      <action name="Desktop"/>
    </keybind>
    <keybind key="W-2">
      <action name="Desktop">
	<desktop>2</desktop>
      </action>
    </keybind>
    <keybind key="W-3">
      <action name="Desktop">
	<desktop>3</desktop>
      </action>
    </keybind>
    <keybind key="W-4">
      <action name="Desktop">
	<desktop>4</desktop>
      </action>
    </keybind>
    <keybind key="W-5">
      <action name="Desktop">
	<desktop>5</desktop>
      </action>
    </keybind>
    <keybind key="W-6">
      <action name="Desktop">
	<desktop>6</desktop>
      </action>
    </keybind>
    <keybind key="W-d">
      <action name="Execute">
	<command>rofi -show drun</command>
      </action>
    </keybind>
    <keybind key="W-q">
      <action name="Close"/>
    </keybind>
    <keybind key="A-Tab">
      <action name="NextWindow">
        <dialog>none</dialog>
        <finalactions>
          <action name="Focus"/>
          <action name="Raise"/>
          <action name="Unshade"/>
        </finalactions>
      </action>
    </keybind>
    <keybind key="W-b">
      <action name="Execute">
	<command>firefox</command>
	<startupnotify>
	  <enabled>yes</enabled>
	  <name>Web Browser</name>
	</startupnotify>
      </action>
    </keybind>
    <keybind key="W-t">
      <action name="Execute">
	<command>alacritty</command>
	<startupnotify>
	  <enabled>yes</enabled>
	  <name>alacritty</name>
	</startupnotify>
      </action>
    </keybind>
    <keybind key="S-A-v">
      <action name="Execute">
        <command>roficlip</command>
      </action>
    </keybind>
    <keybind key="W-S-c">
      <action name="Execute">
        <command>roficonnection</command>
      </action>
    </keybind>
    <keybind key="W-a">
      <action name="ToggleMaximizeFull"/>
    </keybind>
    <keybind key="W-Right">
      <action name="MoveToEdgeEast"/>
    </keybind>
    <keybind key="W-Up">
      <action name="MoveToEdgeNorth"/>
    </keybind>
    <keybind key="W-Left">
      <action name="MoveToEdgeWest"/>
    </keybind>
    <keybind key="W-Down">
      <action name="MoveToEdgeSouth"/>
    </keybind>
    <keybind key="A-W-Up">
      <action name="ResizeRelative">
        <top>5</top>
      </action>
    </keybind>
    <keybind key="A-W-Right">
      <action name="ResizeRelative">
        <right>5</right>
      </action>
    </keybind>
    <keybind key="A-W-Down">
      <action name="ResizeRelative">
        <bottom>5</bottom>
      </action>
    </keybind>
    <keybind key="A-W-Left">
      <action name="ResizeRelative">
        <left>5</left>
      </action>
    </keybind>
    <keybind key="S-W-Up">
      <action name="ResizeRelative">
        <bottom>-5</bottom>
      </action>
    </keybind>
    <keybind key="S-W-Right">
      <action name="ResizeRelative">
        <left>-5</left>
      </action>
    </keybind>
    <keybind key="S-W-Down">
      <action name="ResizeRelative">
        <top>-5</top>
      </action>
    </keybind>
    <keybind key="S-W-Left">
      <action name="ResizeRelative">
        <right>-5</right>
      </action>
    </keybind>
    <keybind key="W-c">
      <action name="MoveToCenter"/>
    </keybind>
    <keybind key="XF86AudioRaiseVolume">
	<action name="Execute">
	  <command>amixer set Master 5%+ unmute</command>
	</action>
    </keybind>
    <keybind key="XF86AudioLowerVolume">
      <action name="Execute">
	<command>amixer set Master 5%- unmute</command>
      </action>
    </keybind>
    <keybind key="XF86AudioMute">
      <action name="Execute">
	<command>amixer set Master toggle</command>
      </action>
    </keybind>
    <keybind key="XF86MonBrightnessUp">
      <action name="Execute">
	<command>xbacklight -inc 10</command>
      </action>
    </keybind>
    <keybind key="XF86MonBrightnessDown">
      <action name="Execute">
	<command>xbacklight -dec 10</command>
      </action>
    </keybind>
    <keybind key="S-W-5">
      <action name="Execute">
        <command>screenshot</command>
      </action>
    </keybind>
  </keyboard>
  <mouse>
    <dragThreshold>1</dragThreshold>
    <doubleClickTime>500</doubleClickTime>
    <screenEdgeWarpTime>0</screenEdgeWarpTime>
    <context name="Titlebar">
      <mousebind button="Left" action="Drag">
	<action name="Move"/>
      </mousebind>
    </context>
  </mouse>
  <applications>
    <application class="*">
      <position>
	<monitor>1</monitor>
      </position>
      <focus>yes</focus>
      <maximized>no</maximized>
    </application>
  </applications>
</openbox_config>
