*font: xft:Roboto Mono:size=12, xft:DejaVu Sans:size=12
XTerm.vt100.faceName: xft:Roboto Mono:size=12, xft:DejaVu Sans:size=12

Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

XTerm*faceName:          Fixed
XTerm*faceSize:          11
XTerm*reverseVideo:      on
XTerm*selectToClipboard: true

Xcursor.color: #ff8300
Xcursor.theme: xcursor-breeze
Xcursor.size:  0

URxvt.scrollBar: false
URxvt.font: xft:Roboto Mono:size=12, xft:DejaVu Sans:size=12
URxvt.lineSpace: 2.5

rofi.font: Roboto 16